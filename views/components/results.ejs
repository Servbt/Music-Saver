<!-- Display for each item recieved from Youtube API from POST request made clientside by srchForm.ejs -->
<body class="row">
    <% content.forEach(video=> { %>
        <div class="video-link video-card my-5 py-2 col bg-dark rounded-4 border-5">
                <h1 class="srch-res fs-3 ps-5"><%=video.snippet.title %></h1>
                <img class="ps-5" src="<%=video.snippet.thumbnails.medium.url %>" alt="<%=video.snippet.title %>">
                <h2 class="srch-res fs-6 ps-5"><%=video.id.videoId%></h2>
        </div>
        <% }); %>
        <%- include("videoFind.ejs") %>
</body>
